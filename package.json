{
  "name": "acquia-dam-sdk",
  "version": "1.0.0",
  "description": "Interact with the Acquia DAM API",
  "license": "ISC",
  "author": "Acquia",
  "repository": "github:Widen/acquia-dam-api-sdk",
  "keywords": [
    "acquia",
    "dam",
    "digital asset manager",
    "integration",
    "widen"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./apis": {
      "import": "./dist/apis/index.mjs",
      "require": "./dist/apis/index.cjs",
      "types": "./dist/apis/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.cjs",
      "types": "./dist/client/index.d.ts"
    },
    "./events/*": {
      "types": "./dist/events/*.d.ts"
    },
    "./*/requests": {
      "types": "./dist/apis/*/requests.d.ts"
    },
    "./*/responses": {
      "types": "./dist/apis/*/responses.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:code & yarn build:docs",
    "build:docs": "yarn typedoc",
    "build:code": "yarn rollup -c",
    "clean": "rm -rf dist doc",
    "format": "prettier -w src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "models": "./scripts/generate-models.sh",
    "precommit": "yarn models && (yarn ts & yarn lint & yarn format) && yarn test",
    "test": "jest",
    "test:ci": "jest --config=jest.config-ci.ts",
    "test:verbose": "jest --silent=false",
    "ts": "tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.2",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-widen": "^4.0.3",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-sort": "^4.0.0",
    "eslint-plugin-widen": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rollup": "^4.28.1",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.2",
    "ts-to-zod": "^3.15.0",
    "typedoc": "^0.27.5",
    "typedoc-github-theme": "^0.2.0",
    "typedoc-plugin-missing-exports": "^3.1.0",
    "typedoc-plugin-rename-defaults": "^0.7.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.18.1",
    "zod": "^3.24.1"
  },
  "type": "module",
  "prettier": "./prettier.config.js",
  "packageManager": "yarn@4.5.3"
}
